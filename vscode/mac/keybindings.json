// 既定値を上書きするには、このファイル内にキー バインドを挿入します
[
    {
        "key": "shift+cmd+m",
        "command": "markdown.showPreview",
        "when": "editorLangId == 'markdown'"
    },
    {
        "key": "shift+cmd+v",
        "command": "-markdown.showPreview",
        "when": "editorLangId == 'markdown'"
    },
    {
      "key": "cmd+,",
      "command": "workbench.action.openSettingsJson"
    }
]